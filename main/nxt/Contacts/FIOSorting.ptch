appfile <ContactsProvider.apk>;

# Sorting by FIO with switcher in contacts settings
replaceinfile "%smali%/com/android/providers/contacts/ContactsDatabaseHelper.smali" 		"const/16 v28, 0x1\n\n    const/16 v29, 0x1" 			"move-object/from16 v0, p0\n\n    :try_start_10\n\n    iget-object v1, v0, Lcom/android/providers/contacts/ContactsDatabaseHelper;->mContext:Landroid/content/Context;\n\n    invoke-virtual {v1}, Landroid/content/Context;->getContentResolver()Landroid/content/ContentResolver;\n\n    move-result-object v1\n\n    const-string v2, "android.contacts.DISPLAY_ORDER"\n\n    invoke-static {v1, v2}, Landroid/provider/Settings$System;->getInt(Landroid/content/ContentResolver;Ljava/lang/String;)I\n\n    move-result v1\n\n    :try_end_10\n    .catchall {:try_start_10 .. :try_end_10} :catchall_10\n\n    const/16 v2, 0x1\n\n    if-ne v1, v2, :cond_50\n\n    const/16 v28, 0x1\n\n    goto :goto_50\n\n    :catchall_10\n\n    const/16 v28, 0x1\n\n    goto :goto_50\n\n    :cond_50\n\n    const/16 v28, 0x0\n\n    :goto_50\n\n    const/16 v29, 0x1";
